#include <iostream>
#include<cmath>
using namespace std;
int main()
{
    int n,i,x=0;
    cout<<"Enter the number: ";
    cin>>n;
    for(i=2;i<=sqrt(n);i++)
    {
        if(n%i==0)
        {
            cout<<"The number is not prime";
            x=1;
            break;
        }
    }
    if(x==0)
        cout<<"The number is prime";
    return 0;
}
