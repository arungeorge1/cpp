#include <iostream>
using namespace std;
int main()
{
    int i,j,rows,columns;
    cout<<"Enter the number of rows: ";
    cin>>rows;
    cout<<"Enter the number of columns: ";
    cin>>columns;
    for(i=1;i<=rows;i++)
    {
        for(j=0;j<columns;j++)
            cout<<"*";
        cout<<"\n";
    }
    return 0;
}
